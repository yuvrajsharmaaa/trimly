{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/dashboard/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/link/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:shortcode",
      "destination": "/api/redirect/:shortcode"
    }
  ]
} 